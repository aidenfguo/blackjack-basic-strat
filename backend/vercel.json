{
    "version":2,
    "builds":[
        {"src": "*.js", "use": "@vercel/node"}
    ],
    "routes":[
        {"src":"/(.*)",
    "dest":"/"
    }
    ],
    "functions": {
        "./index.js": {
          "memory": 3008,
          "maxDuration": 30
        },
        "./*.js": {
          "memory": 3008,
          "maxDuration": 30
        }
      }
}